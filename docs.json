{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "linden",
  "name": "Mint Starter Kit",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Beranda",
        "groups": [
          {
            "group": "Halaman Utama",
            "pages": [
              "beranda"
            ]
          },
          {
            "group": "Standar Pelayanan",
            "pages": [
              "quickstart",
              "development"
            ]
          },
          {
            "group": "Essentials",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/settings",
              "essentials/navigation",
              "essentials/reusable-snippets"
            ]
          }
        ]
      },
      {
        "tab": "Layanan",
        "groups": [
          {
            "group": "Layanan",
            "pages": [
              "layanan/bentuk-layanan",
              "layanan/alur"
            ]
          }
        ]
      },
      {
        "tab": "Survei",
        "groups": [
          {
            "group": "Survei",
            "pages": [
              "survei/hasil",
              "survei/formulir"
            ]
          }
        ]
      },
      {
        "tab": "Tentang Kami",
        "pages": [
          "tentang/tugas-fungsi",
          "tentang/kontak"
        ]
      }
    ]
    
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  
  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "github": "https://github.com/mintlify",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
  },
  "fonts": {
        "heading": {
            "family": "Raleway",
            "weight": 700,
            "source": "https://cdn.jsdelivr.net/fontsource/fonts/raleway:vf@latest/latin-wght-normal.woff2",
            "format": "woff2"
        },
        "body": {
            "family": "Inter",
            "weight": 400,
            "source": "https://cdn.jsdelivr.net/fontsource/fonts/inter@latest/latin-400-normal.woff2",
            "format": "woff2"
        }
    }
}