{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "linden",
  "name": "Mint Starter Kit",
  "appearance": {
    "default": "light"
  },
  "background": {
    "decoration": "grid",
    "color": {"light": "#f5fbff", "dark": "#121212"}
  },
  "colors": {
    "primary": "#0048BA",
    "light": "#007BFF",
    "dark": "#66B2FF"
  },
  "favicon": "/favicon.png",
  "navigation": {
    "tabs": [
      {
        "tab": "Beranda",
        "groups": [
          {
            "group": "Halaman Utama",
            "pages": [
              "beranda"
            ]
          }
        ]
      },
      {
        "tab": "Layanan",
        "groups": [
          {
            "group": "Layanan",
            "pages": [
              "layanan/bentuk-layanan",
              "layanan/alur",
              "layanan/standar-pelayanan"
            ]
          }
        ]
      },
      {
        "tab": "Survei",
        "groups": [
          {
            "group": "Survei",
            "pages": [
              "survei/hasil",
              "survei/formulir"
            ]
          }
        ]
      },
      {
        "tab": "Tentang Kami",
        "pages": [
          "tentang/tugas-fungsi",
          "tentang/kontak"
        ]
      }
    ]
    
  },
  "logo": {
    "light": "/logo/ult-bgp-light.png",
    "dark": "/logo/ult-bgp-dark.png"
  },
  
  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "github": "https://github.com/mintlify",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
  },
  "fonts": {
        "heading": {
            "family": "Raleway",
            "weight": 700,
            "source": "https://cdn.jsdelivr.net/fontsource/fonts/raleway:vf@latest/latin-wght-normal.woff2",
            "format": "woff2"
        },
        "body": {
            "family": "Inter",
            "weight": 400,
            "source": "https://cdn.jsdelivr.net/fontsource/fonts/inter@latest/latin-400-normal.woff2",
            "format": "woff2"
        }
    }
}